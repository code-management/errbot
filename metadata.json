{
  "name": "codemanagement-errbot",
  "version": "0.1.2",
  "author": "codemanagement",
  "summary": "Deploy & manage errbot with Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/code-management/errbot",
  "project_page": "https://github.com/code-management/errbot",
  "issues_url": "https://github.com/code-management/errbot/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.6.0"
    },
    {
      "name": "stankevich-python",
      "version_requirement": ">= 1.12.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ]
}
